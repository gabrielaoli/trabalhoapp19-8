public class Main {
	
public static void main(String[] args) {


//Criando objeto com construtor vazio:
Pessoa pessoa1 = new Pessoa(2, "Carol", 23);

//Criando objeto com parâmetros:
Pessoa pessoa2 = new Pessoa(1, "João", 20);

pessoa2.setNome("José");


System.out.println("O nome da pessoa1 é " + pessoa1.getNome() + ".");

System.out.println("O nome da pessoa2 é " + pessoa2.getNome() + ".");





}
}